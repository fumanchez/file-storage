{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolderBasename}.dll",
      "cwd": "${workspaceFolder}/bin/Debug/net6.0",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}